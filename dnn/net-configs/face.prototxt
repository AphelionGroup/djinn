name: "face"
input: "data"
input_dim: 1
input_dim: 3
input_dim: 152
input_dim: 152
state {
  phase: TEST
}
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data"
  top: "conv1"
  convolution_param {
    num_output: 32
    kernel_size: 11
    stride: 1
  }
}
layer {
  name: "pool2"
  type: "Pooling"
  bottom: "conv1"
  top: "pool2"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "conv3"
  type: "Convolution"
  bottom: "pool2"
  top: "conv3"
  convolution_param {
    num_output: 16
    kernel_size: 9
    stride: 1
  }
}
layer {
  name: "local4"
  type: "Local"
  bottom: "conv3"
  top: "local4"
  local_param {
    num_output: 16
    kernel_size: 9
    stride: 1
  }
}
layer {
  name: "local5"
  type: "Local"
  bottom: "local4"
  top: "local5"
  local_param {
    num_output: 16
    kernel_size: 7
    stride: 2
  }
}
layer {
  name: "local6"
  type: "Local"
  bottom: "local5"
  top: "local6"
  local_param {
    num_output: 16
    kernel_size: 5
    stride: 1
  }
}
layer {
  name: "fc7"
  type: "InnerProduct"
  bottom: "local6"
  top: "fc7"
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "fc8"
  type: "InnerProduct"
  bottom: "fc7"
  top: "fc8"
  inner_product_param {
    num_output: 83
  }
}
layer {
  name: "prob"
  type: "Softmax"
  bottom: "fc8"
  top: "prob"
}
layer {
  name: "argmax"
  type: "ArgMax"
  bottom: "prob"
  top: "argmax"
}
